{
    "functions": {
      "api/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      }
    ],
    "middleware": [
      {
        "source": "/((?!.*\\..*|_next).*)",
        "destination": "/"
      },
      {
        "source": "/(api|trpc)(.*)",
        "destination": "/api"
      }
    ]
  }
  